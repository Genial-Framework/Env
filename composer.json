{
    "name": "genial-framework/env",
    "type": "library",
    "description": "A wrapper for managing the framework configuration details!",
    "homepage": "https://genial.tech/php/genial-framework/components/env",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish0820@outlook.com",
            "homepage": "https://genial.tech/php/genial-framework/authors/nenglish",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Genial": "src/"
        },
        "files": ["main.bootstrap.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
